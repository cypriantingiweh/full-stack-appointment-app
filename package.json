    "express",
    "sequelize",
    "rest",
    "api",
    "postgresql"
  ],
  "author": "Cyprian Tingiweh",
  "license": "CTS",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "logrocket": "^3.0.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.1"
  },
  "proxy": "http://localhost:8080",
  "devDependencies": {
    "concurrently": "^7.5.0"
  }
